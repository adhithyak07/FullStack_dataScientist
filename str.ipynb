    "str=input()\n",
    "x=str.split()\n",
    "words=len(x)\n",
    "char=len(str)\n",
    "set={'a','e','i','o','u','A','E','I','O','U'}\n",
    "v=0\n",
    "for i in x:\n",
    "    for j in i:\n",
    "        if j.isalpha():\n",
    "            if j in set:\n",
    "                v=v+1\n",
    "          \n",
    "print(words,char,v)"
   ]
