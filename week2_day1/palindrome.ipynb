    "def ispalindrome(s):\n",
    "    s=s.lower().strip()\n",
    "    i=0\n",
    "    j=len(s)-1\n",
    "    while(i<j):\n",
    "        if(s[i]==s[j]):\n",
    "            i=i+1\n",
    "            j=j-1\n",
    "        else:\n",
    "            return False\n",
    "    return True\n",
    "text=input()\n",
    "print(ispalindrome(text))"
   ]
